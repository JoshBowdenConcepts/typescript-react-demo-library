        "react-dom": "^17.0.2",
        "styled-components": "^5.3.3"
    },
    "devDependencies": {
        "@types/react": "^16.9.49",
        "@types/react-dom": "^17.0.11",
        "@types/styled-components": "^5.1.18",
        "styled-components": "^5.3.3",
        "typescript": "^4.0.2"
    },
    "files": [
        "/lib"
    ]
